{
  "name": "cpam1410",
  "version": "1.0.0",
  "description": "Dockerized Wordpress application for CPAM1410",
  "main": "index.js",
  "scripts": {
    "dev-up": "docker-compose -f compose/docker-compose.dev.yml up -d",
    "dev-down": "docker-compose -f compose/docker-compose.dev.yml down",
    "build-up": "docker-compose -f compose/docker-compose.build.yml up --build",
    "build-down": "docker-compose -f compose/docker-compose.build.yml down",
    "prod-up": "docker-compose -f compose/docker-compose.prod.yml up -d",
    "prod-down": "docker-compose -f compose/docker-compose.prod.yml down",
    "swarm-up": "docker swarm init && docker stack deploy -c compose/docker-compose.stack.yml cpam1410",
    "swarm-down": "docker swarm init && docker stack deploy -c compose/docker-compose.stack.yml cpam1410"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/max2-wms/CPAM1410.git"
  },
  "author": "Maxime Pierre <max@webmediasolutionz.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/max2-wms/CPAM1410/issues"
  },
  "homepage": "https://github.com/max2-wms/CPAM1410#readme"
}
